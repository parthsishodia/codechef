/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int plane=10;
		for(int i=0;i<n;i++)
		{
		    int cap=sc.nextInt();
		    int peo=sc.nextInt();
		    int cost=sc.nextInt();
		    if(cap*plane>=peo)
		    {
		        System.out.println(peo*cost);
		    }
		    else{
		        System.out.println(cap*10*cost);
		    }
		}
	}
}

