import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
	
		for(int i=0;i<n;i++)
		{   
		    	int res=0;
		    int dis=sc.nextInt();
		    res=res+(dis+dis)*5;
		    System.out.println(res);
		    
		}
	}
