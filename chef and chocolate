/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
		    int x=sc.nextInt();
		    int y=sc.nextInt();
		    int cost=sc.nextInt();
		    int valx=5*x;
		    int valy=y*10;
		    int total=valx+valy;
		    int choco=total/cost;
		    System.out.println(choco);
		}
	}
}
