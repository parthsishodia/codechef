import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		    int n=sc.nextInt();
		    int ans=1;
		    for(int i=0;i<n;i++){
		        int num=sc.nextInt();
		        ans=Math.max(ans,num);
		    }
		    System.out.println(ans);
		}
	}
}
